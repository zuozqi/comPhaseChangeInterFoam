FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 1e-4;

boundaryField
{
    ".*wall"
    {
        type            kqRWallFunction;
        value           $internalField;
    }


    defaultFaces
    {
        type            empty;
    }
}


