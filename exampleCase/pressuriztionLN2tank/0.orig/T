FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 76.8;

boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform 81;
    }

    defaultFaces
    {
        type            empty;
    }
}

